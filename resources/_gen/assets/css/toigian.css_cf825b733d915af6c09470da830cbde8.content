/* #         Overall design        # */
/* ################################# */
h1,
h2,
h3,
h4,
h5,
h6 {
  padding-top: 1.5rem;
  padding-bottom: 1rem;
  font-weight: 700;
  letter-spacing: -0.025em;
}
h1 {
  margin-bottom: 2rem;
  font-size: 2.25rem;
}
h2 {
  margin-top: 3rem;
  margin-bottom: 1.5rem;
  font-size: 1.875rem;
  counter-increment: h2;
  counter-reset: h3;
}
h3 {
  margin-top: 2rem;
  margin-bottom: 1rem;
  font-size: 1.5rem;
  counter-increment: h3;
  counter-reset: h4;
}
h4 {
  margin-top: 1.5rem;
  margin-bottom: 1rem;
  font-size: 1.25rem;
  counter-increment: h4;
}
h5 {
  margin-top: 1rem;
  margin-bottom: 0.5rem;
  font-size: 1.125rem;
}
h6 {
  margin-top: 1rem;
  margin-bottom: 0.5rem;
  font-size: 1rem;
  font-weight: 600;
  color: var(--base);
}
/* nested counters */
h2::before {
  margin-right: 0.5rem;
  margin-left: -0.75rem;
  color: var(--muted);
  content: counter(h2) ". ";
}
h3::before {
  margin-right: 0.5rem;
  margin-left: -0.75rem;
  color: var(--muted);
  content: counter(h2) "." counter(h3) ". ";
}
h4::before {
  margin-right: 0.5rem;
  margin-left: -0.75rem;
  color: var(--muted);
  content: counter(h2) "." counter(h3) "." counter(h4) ". ";
}
h5::before {
  content: counter(h2) "." counter(h3) "." counter(h4) "." counter(h5) ". ";
}
[id] {
  scroll-margin-top: var(--header-height);
}
p {
  padding-top: 0.5rem;
  padding-bottom: 0.5rem;
  line-height: 1.625;
}
strong {
  font-weight: 600;
}
ul > :not([hidden]) ~ :not([hidden]),
ol > :not([hidden]) ~ :not([hidden]) {
