  border-color: var(--muted);
  padding: 1rem;
  --tw-shadow: 0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1);
  --tw-shadow-colored: 0 4px 6px -1px var(--tw-shadow-color), 0 2px 4px -2px var(--tw-shadow-color);
  box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow);
}
pre > code {
  /* Disable inline code style */
  border-radius: 0;
  border-width: 0px;
  background-color: transparent;
  padding: 0px;
  font-family: var(--font-mono), ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, "Liberation Mono", "Courier New", monospace;
  font-size: 15px;
  --tw-shadow: 0 0 #0000;
  --tw-shadow-colored: 0 0 #0000;
  box-shadow: var(--tw-ring-offset-shadow, 0 0 #0000), var(--tw-ring-shadow, 0 0 #0000), var(--tw-shadow);
}
/* ################################# */
/* #       Table of contents       # */
/* ################################# */
@media screen and (min-width: 1080px) {
  #toc {
    position: fixed;
    top: var(--sidebar-top);
    left: var(--page-gutter);
    z-index: 40;
    height: 75%;
    width: 25%;
    overflow: auto;
    padding-left: var(--page-gutter);
    padding-right: var(--page-gutter);
    padding-top: var(--sidebar-top);
    padding-bottom: var(--sidebar-top);
  }
}
#toc summary {
  font-weight: 700;
}
#toc-list ol li {
  list-style-position: outside;
}
#toc-list ol {
  margin: 0.25rem;
  border-left-width: 2px;
  padding-left: 0.75rem;
  font-size: 0.875rem;
  color: var(--muted);
  counter-reset: tocitem;
}
#toc-list > #TableOfContents > ol {
  border-width: 0px;
}
#toc-list a {
  display: inline-block;
  width: 100%;
  border-radius: 0.375rem;
  padding-left: 0.75rem;
  padding-right: 0.75rem;
  line-height: 1.625;
  transition-property: none;
}
#toc-list ol > li {
  margin-top: 0.25rem;
  margin-bottom: 0.25rem;
  display: block;
  counter-increment: tocitem;
}
#toc-list li > a::before {
  margin-right: 0.5rem;
  margin-left: -0.75rem;
  font-weight: 600;
  content: counters(tocitem, ".") ".";
}
#toc-list li > a:hover {
  color: var(--text);
}
#toc-list li > a > code {
  font-size: 0.75rem;
}
/* ###################### */
/* #       Figure       # */
/* ###################### */
figure img {
  margin-left: auto;
  margin-right: auto;
  display: block;
  border-radius: 0.75rem;
  --tw-drop-shadow: drop-shadow(0 10px 8px rgb(0 0 0 / 0.04)) drop-shadow(0 4px 3px rgb(0 0 0 / 0.1));
  filter: var(--tw-blur) var(--tw-brightness) var(--tw-contrast) var(--tw-grayscale) var(--tw-hue-rotate) var(--tw-invert) var(--tw-saturate) var(--tw-sepia) var(--tw-drop-shadow);
}
figure figcaption {
  padding-left: 1.25rem;
  padding-right: 1.25rem;
  padding-top: 0.75rem;
  padding-bottom: 0.75rem;
  text-align: center;
  font-style: italic;
  color: var(--muted);
}
/* ################### */
/* #       Tag       # */
/* ################### */
#tag {
  padding: 0.25rem;
  font-style: italic;
  color: var(--muted);
  text-decoration-line: underline;
}
#tag:before,
#tag:after {
  display: inline-block;
  color: var(--muted);
  text-decoration-line: none;
}
#tag:before {
  content: "[#";
}
#tag:after {
  content: "]";
}
/* ##################### */
/* #       Links       # */
/* ##################### */
.content a {
  padding: 0.25rem;
  font-weight: 500;
  font-style: italic;
}
.content a::before,
  .content a::after {
  display: inline-block;
  color: var(--muted);
  text-decoration-line: none;
}
.content a::before {
    content: "{";
    padding-right: 0.25rem;
  }
.content a::after {
    content: "}";
    padding-left: 0.25rem;
  }
.content a:hover {
  text-decoration-line: underline;
  text-decoration-color: var(--muted);
  text-underline-offset: 2px;
}
/* ##################### */
/*         Table         */
/* ##################### */
.content table {
  margin-top: 1rem;
  margin-bottom: 1rem;
  display: block;
  min-width: 100%;
}
.content table > :not([hidden]) ~ :not([hidden]) {
  --tw-divide-y-reverse: 0;
  border-top-width: calc(2px * calc(1 - var(--tw-divide-y-reverse)));
  border-bottom-width: calc(2px * var(--tw-divide-y-reverse));
}
.content table {
  overflow: auto;
  font-size: 0.875rem;
}
.content table thead:where([dir="ltr"], [dir="ltr"] *) {
  text-align: left;
}
.content table thead:where([dir="rtl"], [dir="rtl"] *) {
  text-align: right;
}
.content table tbody > :not([hidden]) ~ :not([hidden]) {
  --tw-divide-y-reverse: 0;
  border-top-width: calc(1px * calc(1 - var(--tw-divide-y-reverse)));
  border-bottom-width: calc(1px * var(--tw-divide-y-reverse));
}
.content tr th {
  font-weight: 500;
}
.content tr th,
  .content tr td {
  padding-left: 1rem;
  padding-right: 1rem;
  padding-top: 0.5rem;
  padding-bottom: 0.5rem;
}
.content img {
  margin-top: 1rem;
  margin-bottom: 1rem;
}
.content ul {
  margin-left: 1.5rem;
  list-style-type: disc;
}
.content ol {
  margin-left: 1.5rem;
  list-style-type: decimal;
}
/* Custom defined styles */
/* You can add custom styles here. */
.before\:absolute::before {
  content: var(--tw-content);
  position: absolute;
}
.before\:inset-0::before {
  content: var(--tw-content);
  inset: 0px;
}
.before\:z-\[-1\]::before {
  content: var(--tw-content);
  z-index: -1;
}
.before\:bg-base::before {
  content: var(--tw-content);
  background-color: var(--base);
}
.hover\:underline:hover {
  text-decoration-line: underline;
}
.focus\:underline:focus {
  text-decoration-line: underline;
}
.focus\:outline-none:focus {
  outline: 2px solid transparent;
  outline-offset: 2px;
}
@media (min-width: 640px) {
  .sm\:space-y-20 > :not([hidden]) ~ :not([hidden]) {
    --tw-space-y-reverse: 0;
    margin-top: calc(5rem * calc(1 - var(--tw-space-y-reverse)));
    margin-bottom: calc(5rem * var(--tw-space-y-reverse));
  }
}
@media (min-width: 768px) {
  .md\:block {
    display: block;
  }
}
@supports(backdrop-filter:blur(0)) {
  .\[\@supports\(backdrop-filter\:blur\(0\)\)\]\:before\:bg-base::before {
    content: var(--tw-content);
    background-color: var(--base);
  }
  .\[\@supports\(backdrop-filter\:blur\(0\)\)\]\:before\:backdrop-blur-md::before {
    content: var(--tw-content);
    --tw-backdrop-blur: blur(12px);
    -webkit-backdrop-filter: var(--tw-backdrop-blur) var(--tw-backdrop-brightness) var(--tw-backdrop-contrast) var(--tw-backdrop-grayscale) var(--tw-backdrop-hue-rotate) var(--tw-backdrop-invert) var(--tw-backdrop-opacity) var(--tw-backdrop-saturate) var(--tw-backdrop-sepia);
    backdrop-filter: var(--tw-backdrop-blur) var(--tw-backdrop-brightness) var(--tw-backdrop-contrast) var(--tw-backdrop-grayscale) var(--tw-backdrop-hue-rotate) var(--tw-backdrop-invert) var(--tw-backdrop-opacity) var(--tw-backdrop-saturate) var(--tw-backdrop-sepia);
  }
}
